{
  "name": "fabletopia-client",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "run-p dev gql:start",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "gql:start": "nodemon --watch \"src/data-access/**/*\" -e gql --exec \"yarn gql:codegen\"",
    "gql:schema": "get-graphql-schema $(grep VITE_SERVER_URL .env | cut -d '=' -f2) > src/data-access/schema.gql",
    "gql:codegen": "npx graphql-codegen --config src/data-access/codegen.yml"
  },
  "dependencies": {
    "@apollo/client": "3.7.9",
    "@chakra-ui/react": "2.5.1",
    "@emotion/react": "11.10.6",
    "@emotion/styled": "11.10.6",
    "@types/react-gtm-module": "2.0.1",
    "framer-motion": "10.1.0",
    "graphql": "16.6.0",
    "i18next": "22.4.10",
    "react": "18.2.0",
    "react-device-detect": "2.2.3",
    "react-dom": "18.2.0",
    "react-gtm-module": "2.0.11",
    "react-helmet": "6.1.0",
    "react-i18next": "12.2.0",
    "react-icons": "4.7.1",
    "react-router-dom": "6.8.2",
    "screenfull": "6.0.2",
    "socket.io-client": "4.6.1",
    "typesafe-actions": "5.1.0",
    "vite-plugin-favicons-inject": "2.2.0"
  },
  "devDependencies": {
    "@graphql-codegen/add": "4.0.1",
    "@graphql-codegen/cli": "3.2.1",
    "@graphql-codegen/typescript-operations": "3.0.1",
    "@graphql-codegen/typescript-react-apollo": "3.3.7",
    "@graphql-eslint/eslint-plugin": "3.16.1",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "@types/react-helmet": "6.1.6",
    "@vitejs/plugin-react": "3.1.0",
    "get-graphql-schema": "2.1.2",
    "nodemon": "2.0.20",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.4",
    "typescript": "4.9.3",
    "vite": "4.1.0",
    "vite-tsconfig-paths": "4.0.5"
  }
}
